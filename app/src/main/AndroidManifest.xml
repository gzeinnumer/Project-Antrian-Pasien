<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.riska.riskatulmahmudah.antrianpasien">

    <!-- to do 1 gunanya supaya bisa akses ke internet -->
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.PerawatActivity" />
        <activity android:name=".activity.PasienActivity" />
        <activity android:name=".activity.SignInActivity" />
        <activity android:name=".activity.DokterActivity"></activity>
    </application>

</manifest>